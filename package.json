{
  "name": "r2-uploader",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vercel dev -l 7896",
    "dev": "vite",
    "build:setup": "cp README.md public/setup-guide/index.md; bun run public/setup-guide/gen_markdown.js",
    "build": "bun run build:setup; vite build",
    "preview": "vite preview",
    "deploy": "bun run build:setup; vercel --prod",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "bun-types": "^1.1.29",
    "compressorjs": "^1.2.1",
    "crypto-js": "^4.2.0",
    "marked": "^9.1.6",
    "mongodb": "^6.9.0",
    "nanoid": "^4.0.2",
    "pinia": "^2.2.2",
    "postcss": "^8.4.47",
    "sass": "^1.79.3",
    "tailwindcss": "^3.4.13",
    "vue": "^3.5.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "vite": "^5.4.8"
  }
}
